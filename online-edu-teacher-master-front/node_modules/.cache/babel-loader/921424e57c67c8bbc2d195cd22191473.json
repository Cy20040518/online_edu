_createForOfIteratorHelper","_step","s","n","done","value","err","e","f"],"sources":["src/views/course_add/create_info.vue"],"sourcesContent":["<!-- 步骤1 ： 创建课程基本信息 -->\n<template>\n  <div>\n    <el-form :model=\"data\" size=\"small\" label-width=\"100px\">\n      <el-form-item label=\"课程封面：\" prop=\"cover\">\n        <el-upload\n          action=\"\"\n          :auto-upload=\"false\"\n          :show-file-list=\"false\"\n          accept=\".png, .jpg, .jpeg\"\n          :on-change=\"selectAvatar\"\n        >\n          <el-tooltip effect=\"dark\" content=\"点击上传海报，图片不能超过2MB\" placement=\"right\">\n            <el-image :src=\"encodeOssFileUri(original.cover)\" fit=\"cover\" class=\"cover-img\">\n              <div slot=\"error\" class=\"image-slot\">\n                <i class=\"el-icon-picture-outline\" />\n              </div>\n            </el-image>\n          </el-tooltip>\n        </el-upload>\n      </el-form-item>\n      <el-form-item label=\"课程名称：\" prop=\"title\">\n        <el-input v-model=\"data.title\" placeholder=\"请输入课程名称\" />\n      </el-form-item>\n      <el-form :model=\"data\" :inline=\"true\" size=\"small\" label-width=\"100px\">\n        <el-form-item label=\"价　　格：\" prop=\"price\">\n          <el-input-number v-model=\"data.price\" :precision=\"2\" :min=\"0\" style=\"width: 200px\" />\n        </el-form-item>\n        <el-form-item label=\"课程分类：\" prop=\"subjectId\">\n          <el-cascader\n            v-model=\"data.subjectId\"\n            :options=\"subjectOptions\"\n            :props=\"{ expandTrigger: 'hover', value: 'id', label: 'title' }\"\n            :clearable=\"true\"\n            :filterable=\"true\"\n            placeholder=\"请选择课程分类\"\n            style=\"width: 360px\"\n            @change=\"subjectOptionsChange\"\n          />\n        </el-form-item>\n      </el-form>\n      <div>\n        <p class=\"into\">课程简介：</p>\n        <div id=\"editor\" />\n      </div>\n    </el-form>\n  </div>\n</template>\n\n<script>\n\nimport { getInfo, uploadPic } from '@/api/course'\nimport { getSubject } from '@/api/subject'\nimport { encodeOssFileUri, jsonObj2FormData } from '@/utils'\nimport E from 'wangeditor'\nimport store from '@/store'\n\nexport default {\n  name: 'CreateInfo',\n  data() {\n    return {\n      original: {\n        cover: ''\n      },\n      data: {\n        id: 0,\n        title: '',\n        description: '',\n        price: 0.00,\n        subjectId: null\n      },\n      editor: null,\n      // 分类选项\n      subjectOptions: []\n    }\n  },\n  created() {\n    this.$nextTick(() => {\n      this.initEditor()\n    })\n    this.getSubject()\n  },\n  methods: {\n    setData(data) {\n      getInfo(data.id).then(resp => {\n        this.original = resp.data\n        Object.keys(this.data).forEach((key) => {\n          this.data[key] = this.original[key]\n        })\n        this.editor.txt.html(this.data.description)\n      })\n    },\n    getData() {\n      this.data.teacherId = store.getters.user.id\n      return jsonObj2FormData(this.data)\n    },\n    encodeOssFileUri(ossUri) {\n      return encodeOssFileUri(ossUri)\n    },\n    // 选择图片事件\n    selectAvatar(file) {\n      this.original.cover = URL.createObjectURL(file.raw)\n      this.data.file = file.raw\n    },\n    getSubject() {\n      getSubject().then(resp => {\n        this.subjectOptions = resp.data\n      })\n    },\n    subjectOptionsChange(val) {\n      const arr = val\n      if (!!arr || arr.length > 0) {\n        this.data.subjectId = arr[arr.length - 1]\n      }\n    },\n    // 初始化编辑器\n    initEditor() {\n      const editor = this.editor = new E('#editor')\n      editor.config.height = 200\n      editor.config.zIndex = 1600\n      editor.config.onchange = this.contentChange\n      // 菜单\n      editor.config.excludeMenus = ['emoticon', 'video', 'fontName', 'todo', 'code']\n      // 图片\n      editor.config.uploadImgMaxSize = 2 * 1024 * 1024 // 2M\n      editor.config.uploadImgAccept = ['jpg', 'jpeg', 'png']\n      editor.config.uploadImgMaxLength = 1\n      editor.config.showLinkImg = false\n      editor.config.customUploadImg = this.insertPicToEditor\n      // 创建\n      editor.create()\n    },\n    contentChange(html) {\n      this.data.description = html\n    },\n    insertPicToEditor(files, insertImgFn) {\n      for (const file of files) {\n        uploadPic(jsonObj2FormData({ file: file })).then(resp => {\n          insertImgFn(resp.data)\n        })\n      }\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.block {\n  display: block;\n}\n\n.inliblock {\n  display: inline-block;\n}\n\n.cover-img {\n  width: 360px;\n  height: 120px;\n  background-color: #f5f7fa;\n  line-height: 120px;\n}\n\n.into {\n  width: 100px;\n  text-align: right;\n  padding-right: 12px;\n  margin: 0 0 10px 0;\n  font-size: 14px;\n  color: #606266;\n}\n\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmDA,SAAAA,OAAA,EAAAC,SAAA;AACA,SAAAC,UAAA,IAAAA,WAAA;AACA,SAAAC,gBAAA,IAAAA,iBAAA,EAAAC,gBAAA;AACA,OAAAC,CAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,QAAA;QACAC,KAAA;MACA;MACAF,IAAA;QACAG,EAAA;QACAC,KAAA;QACAC,WAAA;QACAC,KAAA;QACAC,SAAA;MACA;MACAC,MAAA;MACA;MACAC,cAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IAAA,IAAAC,KAAA;IACA,KAAAC,SAAA;MACAD,KAAA,CAAAE,UAAA;IACA;IACA,KAAAnB,UAAA;EACA;EACAoB,OAAA;IACAC,OAAA,WAAAA,QAAAf,IAAA;MAAA,IAAAgB,MAAA;MACAxB,OAAA,CAAAQ,IAAA,CAAAG,EAAA,EAAAc,IAAA,WAAAC,IAAA;QACAF,MAAA,CAAAf,QAAA,GAAAiB,IAAA,CAAAlB,IAAA;QACAmB,MAAA,CAAAC,IAAA,CAAAJ,MAAA,CAAAhB,IAAA,EAAAqB,OAAA,WAAAC,GAAA;UACAN,MAAA,CAAAhB,IAAA,CAAAsB,GAAA,IAAAN,MAAA,CAAAf,QAAA,CAAAqB,GAAA;QACA;QACAN,MAAA,CAAAR,MAAA,CAAAe,GAAA,CAAAC,IAAA,CAAAR,MAAA,CAAAhB,IAAA,CAAAK,WAAA;MACA;IACA;IACAoB,OAAA,WAAAA,QAAA;MACA,KAAAzB,IAAA,CAAA0B,SAAA,GAAA5B,KAAA,CAAA6B,OAAA,CAAAC,IAAA,CAAAzB,EAAA;MACA,OAAAP,gBAAA,MAAAI,IAAA;IACA;IACAL,gBAAA,WAAAA,iBAAAkC,MAAA;MACA,OAAAlC,iBAAA,CAAAkC,MAAA;IACA;IACA;IACAC,YAAA,WAAAA,aAAAC,IAAA;MACA,KAAA9B,QAAA,CAAAC,KAAA,GAAA8B,GAAA,CAAAC,eAAA,CAA